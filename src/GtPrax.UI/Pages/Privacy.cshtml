@page
@inject IOptions<PageContentOptions> pageContent
@model PrivacyModel
@{
	ViewData["Title"] = "Datenschutz";
}

@await Html.PartialAsync("_Header")

<section class="section has-footer">
	<div class="container">
		@await Html.ReadTextContent(pageContent.Value.PrivacyFile)
	</div>
</section>

@await Html.PartialAsync("_Footer")
